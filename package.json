{
  "name": "delivery-much-challenge",
  "version": "1.0.0",
  "description": "A code challenge make to Delivery Much company",
  "main": "src/server.ts",
  "scripts": {
    "build": "tsc",
    "start": "node --inspect=5858 -r ts-node/register ./src/server.ts",
    "start:dev": "nodemon",
    "test": "npx jest --verbose",
    "test:watchAll": "npx jest --watchAll --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wricke/delivery-much-challenge.git"
  },
  "keywords": [
    "typescript",
    "javascript",
    "docker",
    "ts",
    "js",
    "delivery_much",
    "bank",
    "financial",
    "dijkstra"
  ],
  "author": "Wesley Matos <wrickee@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wricke/challenge-delivery_much/issues"
  },
  "homepage": "https://github.com/wricke/challenge-delivery_much#readme",
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.18",
    "@types/node": "^14.14.11",
    "@types/ramda": "^0.27.33",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "eslint": "^7.15.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.2.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.1",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "http-status-codes": "^2.1.4",
    "joi": "^17.3.0",
    "ramda": "^0.27.1",
    "ramda-adjunct": "^2.29.0"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "node -r tsconfig-paths/register -r ts-node/register ./src/server.ts",
    "ext": "ts, js"
  }
}
